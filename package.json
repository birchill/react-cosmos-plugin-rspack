{
  "name": "react-cosmos-plugin-rspack",
  "version": "0.2.0",
  "description": "rspack plugin for React Cosmos",
  "repository": "https://github.com/birchill/react-cosmos-plugin-rspack/",
  "author": "Birchill, Inc.",
  "license": "MIT",
  "main": "./cosmos.plugin.json",
  "files": [
    "dist/**",
    "cosmos.plugin.json"
  ],
  "type": "module",
  "dependencies": {
    "react-cosmos-dom": ">=6.0.0",
    "webpack-dev-middleware": "7.2.1",
    "webpack-hot-middleware": "2.26.1"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "8.0.1",
    "@rspack/core": "1.0.0-beta.5",
    "@types/node": "20.14.9",
    "@types/webpack-hot-middleware": "2.25.9",
    "husky": "9.0.11",
    "knip": "5.23.3",
    "lint-staged": "15.2.7",
    "prettier": "3.3.2",
    "react-cosmos": "6.1.1",
    "react-cosmos-core": "6.1.1",
    "react-error-overlay": "6.0.9",
    "react-plugin": "3.0.0-alpha.4",
    "release-it": "17.4.1",
    "tsup": "8.1.0",
    "typescript": "5.5.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsup && tsup --config tsup.ui.config.ts && cp src/server/rspackConfig/userImportsLoader.cjs dist/server",
    "release": "release-it",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@rspack/core": ">=1.0.0-beta.0",
    "react-cosmos": ">=6.0.0"
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write"
    ]
  }
}
